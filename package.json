{
  "name": "jam3-dev",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/nyg-jam3.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jam3/nyg-jam3/issues"
  },
  "scripts": {
    "dev": "cross-env BUILD_ENV=development next dev",
    "start": "next start",
    "\n ######################  NPM Secondary Pipeline  ######################": "",
    "build:ssr": "next build",
    "build:prod:static": "next build && next export && cross-env CI_ENV=production npm-run-all robots-txt-generator sitemap-generator",
    "build:staging:static": "cross-env CI_ENV=staging next build && next export",
    "build:develop:static": "cross-env CI_ENV=develop next build && next export",
    "ci-prod-server": "serve out &",
    "sitemap-generator": "node scripts/nextjs-sitemap-generator.js",
    "robots-txt-generator": "node scripts/robots-txt-generator.js",
    "\n ######################  Support Development Tools  ######################": "",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "component": "node scripts/component.js",
    "page": "node scripts/component.js --page",
    "\n ######################  Automation Tools  ######################": "",
    "lint-staged": "lint-staged",
    "linters": "npm-run-all -p js-lint sass-lint adviser-dev ls-lint",
    "adviser-dev": "adviser --tags dev --verbose",
    "adviser-ci": "adviser --tags lighthouse,webhint,stage --verbose --quiet",
    "js-lint": "eslint \"./src/**/*.js\" -c ./.eslintrc.json --quiet --ignore-pattern .gitignore",
    "sass-lint": "stylelint \"./src/**/*.scss\"",
    "ls-lint": "node_modules/.bin/ls-lint"
  },
  "dependencies": {
    "@jam3/detect": "1.0.2",
    "@jam3/react-hooks": "1.0.0",
    "classnames": "2.2.6",
    "default-passive-events": "2.0.0",
    "focus-visible": "^5.1.0",
    "get-scroll": "^2.0.1",
    "gsap": "3.5.0",
    "next": "9.5.4",
    "no-op": "1.0.3",
    "normalize.css": "^8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "react-use": "^15.3.3",
    "redux": "^4.0.5",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jam3/react-check-extra-props": "^1.0.1",
    "@jam3/stats": "1.0.1",
    "@ls-lint/ls-lint": "^1.9.2",
    "@next/bundle-analyzer": "9.5.2",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^6.0.21",
    "adviser": "^0.7.5",
    "adviser-plugin-dependencies": "^0.4.2",
    "adviser-plugin-lighthouse": "^1.0.1",
    "adviser-plugin-webhint": "^1.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-preset-react-app": "^9.1.2",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "eslint": "7.6.0",
    "eslint-config-postcss": "4.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-react-jam3": "1.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jam3": "0.2.3",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "4.0.1",
    "generate-robotstxt": "^8.0.3",
    "husky": "^4.2.5",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-pngquant": "9.0.0",
    "imagemin-svgo": "8.0.0",
    "lint-staged": "^10.2.11",
    "maxstache": "^1.0.7",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "next-compose-plugins": "2.2.0",
    "next-images": "^1.4.1",
    "next-optimized-images": "2.6.2",
    "nextjs-sitemap-generator": "^1.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "2.13.8",
    "serve": "^11.3.2",
    "shell-quote": "^1.7.2",
    "storybook-addon-next-router": "^2.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-jam3": "0.1.4",
    "webp-loader": "0.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"Pre-commit checks...\" && lint-staged",
      "pre-push": "echo \"Pre-push checks...\" && npm run linters",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
