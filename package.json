{
  "name": "jam3-dev",
  "version": "9.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run scripts && run-p dev:*",
    "dev:next": "node scripts/dev-server",
    "dev:storybook": "storybook dev --no-open --quiet -p 9001",
    "build:next": "npm run scripts && next build && next export",
    "build:postcss": "postcss ./out/**/*.css --use css-declaration-sorter --replace --no-map",
    "build:sitemap": "next-sitemap",
    "build:storybook": "npm run scripts && storybook build -c .storybook -o ./out-storybook",
    "postbuild:next": "run-p build:postcss build:sitemap",
    "release": "standard-version",
    "scripts": "run-p scripts:*",
    "scripts:assets-imports": "node scripts/assets-imports.js",
    "scripts:public-image-sizes": "node scripts/public-image-sizes.js",
    "lint-ci": "npm run adviser:ci && run-p lint:* tsc",
    "lint-dev": "npm run adviser:dev && run-p lint:* tsc",
    "lint-staged": "lint-staged",
    "lint:ls": "npx @ls-lint/ls-lint",
    "lint:next": "next lint",
    "lint:sass": "stylelint \"./src/**/*.scss\"",
    "adviser:dev": "adviser --tags dev --verbose",
    "adviser:ci": "adviser --tags ci --verbose --quiet",
    "generate": "cross-env NODE_NO_WARNINGS=1 sg wizard",
    "preinstall": "npm i -g husky@8.0.3",
    "prepare": "husky install",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=16.15.1",
    "npm": ">=8.11.0"
  },
  "bugs": {
    "url": "https://github.com/Jam3/nextjs-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/nextjs-boilerplate.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "bowser": "^2.11.0",
    "classnames": "^2.3.2",
    "default-passive-events": "^2.0.0",
    "focus-visible": "^5.2.0",
    "gsap": "file:./gsap-bonus-3.11.3.tgz",
    "js-cookie": "^3.0.5",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "next": "13.4.2",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "string-format": "^2.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ls-lint/ls-lint": "^1.11.2",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/nextjs": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/webpack": "^8.0.1",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/string-format": "^2.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "adviser": "^0.7.9",
    "adviser-plugin-dependencies": "^0.6.0",
    "css-declaration-sorter": "6.3.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "8.0.3",
    "lint-staged": "^13.2.2",
    "next-sitemap": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "storybook": "^7.0.12",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-performance-animation": "^1.2.2",
    "stylelint-prettier": "^3.0.0",
    "typescript": "5.0.4"
  }
}
