{
  "name": "jam3-dev",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/nyg-jam3.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jam3/nyg-jam3/issues"
  },
  "scripts": {
    "dev": "node index.js",
    "start": "next start",
    "\n ######################  NPM Secondary Pipeline  ######################": "",
    "build:ssr": "next build",
    "build:prod:static": "next build && next export && cross-env BUILD_ENV=production npm-run-all robots-txt-generator sitemap-generator",
    "build:staging:static": "cross-env BUILD_ENV=staging next build && next export",
    "sitemap-generator": "node scripts/nextjs-sitemap-generator.js",
    "robots-txt-generator": "node scripts/robots-txt-generator.js",
    "\n ######################  Support Development Tools  ######################": "",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "component": "node scripts/component.js",
    "page": "node scripts/component.js --page",
    "\n ######################  Automation Tools  ######################": "",
    "linters": "npm-run-all -p js-lint sass-lint",
    "js-lint": "eslint './src/**/*.js' -c ./.eslintrc.json --quiet --ignore-pattern .gitignore",
    "sass-lint": "stylelint './src/**/*.scss'",
    "audit": "audit-ci --high"
  },
  "dependencies": {
    "@jam3/detect": "1.0.2",
    "@jam3/react-hooks": "1.0.0",
    "classnames": "2.2.6",
    "default-passive-events": "1.0.10",
    "dompurify": "^2.0.8",
    "get-scroll": "^2.0.0",
    "gsap": "3.1.1",
    "i18next": "^19.1.0",
    "next": "9.2.1",
    "next-i18next": "^4.2.0",
    "no-op": "1.0.3",
    "normalize.css": "^8.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^7.1.3",
    "react-use": "^13.22.4",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@jam3/react-check-extra-props": "^1.0.1",
    "@jam3/stats": "1.0.1",
    "@next/bundle-analyzer": "9.2.1",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@svgr/webpack": "^5.1.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "audit-ci": "^2.4.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.0",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "eslint": "6.8.0",
    "eslint-config-postcss": "3.0.7",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-config-react-jam3": "1.1.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jam3": "0.2.3",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "4.0.1",
    "generate-robotstxt": "^8.0.0",
    "husky": "^4.2.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.0.0",
    "isomorphic-unfetch": "3.0.0",
    "maxstache": "^1.0.7",
    "minimist": "^1.2.0",
    "mkdirp": "^1.0.3",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.0.3",
    "next-images": "^1.3.0",
    "next-offline": "4.0.6",
    "next-optimized-images": "2.5.4",
    "next-server": "^9.0.5",
    "nextjs-sitemap-generator": "^0.5.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "2.13.8",
    "stylelint": "13.0.0",
    "stylelint-config-jam3": "0.1.2",
    "webp-loader": "0.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.5.0"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,json,scss}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Pre-commit checks...' && lint-staged",
      "pre-push": "echo 'Pre-push checks...' && npm run linters && npm run audit",
      "commitmsg": "commitlint -e $GIT_PARAMS"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
